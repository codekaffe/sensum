{
  "name": "sensum",
  "version": "1.2.1",
  "description": "A Discord.JS Bot Framework Written in Typescript.",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node dist/bot.js",
    "test": "tsc --noEmit --pretty && jest && npm run lint && echo \"LOOKS LIKE EVERYTHING IS GOOD! ðŸ’š\"",
    "prepack": "npm run build",
    "build": "rimraf dist && tsc",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint ./src/**/*.ts --fix",
    "deps": "npx npm-check-updates",
    "deps:patch": "npx npm-check-updates -ut patch",
    "deps:minor": "npx npm-check-updates -ut minor",
    "deps:major": "npx npm-check-updates -ut major"
  },
  "keywords": [
    "discord.js",
    "bot",
    "framework",
    "tools",
    "commands"
  ],
  "author": "Code Kaffe",
  "license": "MIT",
  "dependencies": {
    "@discordjs/collection": "^0.3.2",
    "@refabric/extend": "^1.1.4",
    "app-root-path": "^3.0.0",
    "cosmiconfig": "^7.0.1",
    "deep-object-diff": "^1.1.0",
    "discord.js": "^13.3.1",
    "fastest-validator": "^1.12.0",
    "glob": "^7.2.0",
    "lodash.merge": "^4.6.2",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "mri": "^1.2.0",
    "node-schedule": "^2.0.0"
  },
  "devDependencies": {
    "@types/app-root-path": "1.2.4",
    "@types/glob": "7.2.0",
    "@types/jest": "27.0.3",
    "@types/lodash.merge": "4.6.6",
    "@types/moment-duration-format": "2.2.3",
    "@types/mri": "1.1.1",
    "@types/node-schedule": "1.3.2",
    "@types/ws": "8.2.0",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint": "8.2.0",
    "jest": "27.3.1",
    "rimraf": "3.0.2",
    "strict-event-emitter-types": "2.0.0",
    "ts-jest": "27.0.7",
    "typescript": "4.5.2"
  },
  "types": "./dist/index.d.ts"
}
